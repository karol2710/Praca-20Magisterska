    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/node-build.mjs",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
  "key_features": {
    "frontend": [
      "Type-safe React components (TypeScript)",
      "25+ accessible Radix UI components",
      "Utility-first Tailwind CSS styling",
      "Real-time data management (React Query)",
      "Form management (React Hook Form)",
      "Dark/light mode support",
      "Date picker functionality (react-day-picker)",
      "Responsive design"
